{
  "name": "ketone-api",
  "version": "1.0.0",
  "scripts": {
    "test": "bun test --env-file=../.env",
    "test:watch": "bun test --watch --env-file=../.env",
    "test:integration": "bun test --timeout 15000 --env-file=../.env src/features/auth/api/__tests__/auth.integration.test.ts src/features/cycle-v1/api/__tests__/cycle-v1.integration.test.ts src/features/cycle-v1/services/__tests__/cycle-completion-cache.integration.test.ts",
    "test:integration:auth": "bun test --env-file=../.env src/features/auth/api/__tests__/auth.integration.test.ts",
    "test:integration:cycle-v1": "bun test --timeout 15000 --env-file=../.env src/features/cycle-v1/api/__tests__/cycle-v1.integration.test.ts",
    "test:integration:cycle-v1:cache": "bun test --timeout 15000 --env-file=../.env src/features/cycle-v1/services/__tests__/cycle-completion-cache.integration.test.ts",
    "typecheck": "bunx tsc --noEmit",
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:cleanup-test-data": "bun run --env-file=../.env scripts/cleanup-test-data.ts",
    "benchmark:cycles:stress": "bun run --env-file=../.env src/tests/cycle-stress-test.ts",
    "benchmark:cycles:compare": "echo '\\nðŸ“Š Running Postgres benchmark...' && bun run benchmark:cycles:postgres && echo '\\nðŸ“Š Running Redis benchmark...' && bun run benchmark:cycles:redis && echo '\\nâœ… Comparison complete! Review the results above.'"
  },
  "module": "index.ts",
  "type": "module",
  "private": true,
  "dependencies": {
    "@effect/platform": "^0.92.1",
    "@effect/platform-bun": "^0.81.1",
    "@effect/sql": "^0.46.0",
    "@effect/sql-drizzle": "^0.45.0",
    "@effect/sql-pg": "^0.47.0",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.6",
    "effect": "^3.18.4",
    "jose": "^6.1.0",
    "postgres": "^3.4.7",
    "prettier-plugin-organize-imports": "^4.3.0",
    "xstate": "^5.23.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "drizzle-kit": "^0.31.5"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
