{
  "name": "ketone-api",
  "version": "1.0.0",
  "scripts": {
    "test": "bun test --env-file=../.env.local",
    "test:watch": "bun test --watch --env-file=../.env.local",
    "test:integration": "bun test --timeout 15000 --env-file=../.env.local src/features/auth/api/__tests__/auth.integration.test.ts src/features/cycle/api/__tests__/cycle.integration.test.ts src/features/cycle/services/__tests__/cycle-completion-cache.integration.test.ts src/features/cycle/api/__tests__/cycle-statistics.integration.test.ts",
    "test:integration:auth": "bun test --env-file=../.env.local src/features/auth/api/__tests__/auth.integration.test.ts",
    "test:integration:cycle": "bun test --timeout 15000 --env-file=../.env.local src/features/cycle/api/__tests__/cycle.integration.test.ts",
    "test:integration:cycle:cache": "bun test --timeout 15000 --env-file=../.env.local src/features/cycle/services/__tests__/cycle-completion-cache.integration.test.ts",
    "test:integration:cycle:statistics": "bun test --timeout 15000 --env-file=../.env.local src/features/cycle/api/__tests__/cycle-statistics.integration.test.ts",
    "typecheck": "bunx tsc --noEmit",
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "db:generate": "bun --env-file=../.env.local drizzle-kit generate",
    "db:migrate": "bun --env-file=../.env.local drizzle-kit migrate",
    "db:studio": "bun --env-file=../.env.local drizzle-kit studio",
    "db:cleanup-test-data": "bun run --env-file=../.env.local scripts/cleanup-test-data.ts",
    "benchmark:cycles:stress": "bun run --env-file=../.env.local src/tests/cycle-stress-test.ts"
  },
  "module": "index.ts",
  "type": "module",
  "private": true,
  "dependencies": {
    "@effect/cluster": "^0.53.5",
    "@effect/platform": "^0.93.3",
    "@effect/platform-bun": "^0.84.0",
    "@effect/sql": "^0.48.0",
    "@effect/sql-drizzle": "^0.47.0",
    "@effect/sql-pg": "^0.49.6",
    "@ketone/shared": "workspace:*",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.7",
    "effect": "^3.19.6",
    "jose": "^6.1.2",
    "postgres": "^3.4.7",
    "prettier-plugin-organize-imports": "^4.3.0",
    "xstate": "^5.24.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "drizzle-kit": "^0.31.7"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
