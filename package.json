{
  "name": "ketone",
  "version": "2.0.1",
  "private": true,
  "description": "Ketone monorepo - Web, API, and Shared",
  "packageManager": "bun@1.3.4",
  "scripts": {
    "dev:api": "cd api && bun --watch --env-file=../.env.local src/index.ts",
    "dev:web": "cd web && bun run dev",
    "build": "bun run build:all",
    "build:api": "cd api && bun run build",
    "build:web": "cd web && bun run build",
    "typecheck": "bun run typecheck:api && bun run typecheck:web",
    "typecheck:api": "cd api && bun run typecheck",
    "typecheck:web": "cd web && bun run type-check"
  },
  "workspaces": [
    "api",
    "web",
    "shared"
  ],
  "devDependencies": {
    "prettier": "3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0"
  }
}
